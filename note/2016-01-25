clearCache.sh

# !/bin/bash
# 注意，我们这里使用了 "echo 3"，但是不推荐使用在产品环境中，应该使用 "echo 1"
sync && echo 3 > /proc/sys/vm/drop_caches
	
*/10 * * * * /opt/task/clearCache.sh

http://192.168.1.12:9993/jenkins/project/jcommons

clean deploy -P release -Dgpg.passphrase=12345678

configuretion
	配置信息封装

	解析配置信息
		1. 使用xStream插件加载XML配置信息
		2. 缓存sheet标记对应的配置信息
		3. 提供获取模板的接口

		Excel配置解析器
			ExcelConfigParser
				// 加载配置信息
				loadConfig(File config);
				// 缓存配置信息
				cacheConfig();

		Excel配置模板
			ExcelConfigTemplate
			getSheetConfig(sheetID);

		Excel操作接口
			ExcelOperation
			读
				Object read(String sheetId, File excel);
			写
				void write(String sheetId, File excel, List listData);

				void write(String sheetId, File excel, List<Map> listMapData);

				void write(String sheetId, File excel, Map mapData);

				void write(String sheetId, File excel, Map<List> mapListData);

		Excel模板
			ExcelTemplate
		
		Excel总配置文件
			data-transfer-excel.xml


	设计客户端API
		输入
			模板ID
		输出
			Map数据

